  "lint-staged": {
    "**/*.{js,ts,vue}": "prettier --write",
    "*": "eslint --fix"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@nuxt/ui": "^2.20.0",
    "chart.js": "^4.4.6",
    "date-fns": "^4.1.0",
    "nuxt": "^3.13.2",
    "radash": "^12.1.0",
    "steamid": "^2.1.0",
    "uuid": "^11.0.5",
    "vlpt": "^0.0.1",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.6.1",
    "@nuxt/fonts": "^0.10.2",
    "@nuxtjs/i18n": "^8.5.5",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "eslint": "^9.14.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "openapi-typescript": "^7.4.2",
    "prettier": "3.3.3",
    "typescript": "^5.6.3"
  }
